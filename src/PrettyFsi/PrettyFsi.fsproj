<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>PrettyFsi</PackageId>
    <Version>0.0.1-prerelease1</Version>
    <Authors>Ronald Schlenker</Authors>
    <RepositoryUrl>https://github.com/ronaldschlenker/PrettyFsi</RepositoryUrl>
    <PackageTags>fsharp; fsi</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="PrettyFsi.fs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FSharp.Compiler.Service" Version="41.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="FSharp.Compiler.Interactive.Settings">
      <HintPath>..\..\devDependeicies\FSharp.Compiler.Interactive.Settings.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
